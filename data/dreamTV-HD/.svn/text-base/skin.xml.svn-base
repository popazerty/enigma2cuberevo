		<widget name="list" position="0,30" size="550,335" scrollbarMode="showOnDemand" selectionPixmap="dreamTV-HD/menu/select1080_25.png"/>
		<applet type="onLayoutFinish">
# this should be factored out into some helper code, but currently demonstrates applets.
from enigma import eSize, ePoint

orgwidth = self.instance.size().width()
orgpos = self.instance.position()
textsize = self["text"].getSize()

# y size still must be fixed in font stuff...
textsize = (textsize[0] + 80, textsize[1] + 80)
count = len(self.list)
if count &gt; 10:
	count = 10
offset = 25 * count
wsizex = textsize[0] + 80
wsizey = textsize[1] + offset + 10

if (640 &gt; wsizex):
	wsizex = 640
wsize = (wsizex, wsizey)

# resize
self.instance.resize(eSize(*wsize))

# resize label
self["text"].instance.resize(eSize(*textsize))

# move list
listsize = (wsizex - 20, 25 * count)
self["list"].instance.move(ePoint(10, textsize[1]))
self["list"].instance.resize(eSize(*listsize))

# center window
newwidth = wsize[0]
self.instance.move(ePoint((1280-wsizex)/2, (720-wsizey)/(count &gt; 7 and 2 or 3)))

		</applet>
